			<%@repeat=@schedule.activity_repeat.last if !@schedule.activity_repeat.last.nil? %>
			<%@repeat_date=""%>
			<%@repeat_date = @repeat.ends_on.strftime("%Y-%m-%d") if @repeat.present? && @repeat.ends_on.present? %>
