<table cellpadding="0" cellpadding="0" >
  <tr>
    <td style="padding:12px;">
     <table cellpadding="0" cellpadding="0">
        <tr>
            <td style="font-family:Arial, Helvetica, sans-serif; font-size:18px; color:#444444;" height="40"><b>Hello Support Team,</b></td>
        </tr>
        <tr>
            <td style="font-family:Arial, Helvetica, sans-serif; font-size:14px; color:#333333;" height="30">This is to inform you that the following users are due for auto renewal of their accounts today.</td>
        </tr>
	<!--list of auto renewal user details-->
	<tr>
	<td height="10"></td>
	</tr>
	<tr>
		<td height="30" style="color:#307193;"><b>List of Auto-Renewal User Details</b></td>
	</tr>
	<!--loop starting here-->
	<%if @users_list && !@users_list.nil? && @users_list.present? && @users_list!="" && @users_list!=[]%>
		<tr>
			<td style="border:1px solid #D6EDF3" width="500">
				<table cellpadding="0" cellspacing="0" width="90%"  style="margin:0 auto" >
					<tr>
						<td height="30" style="border-bottom:1px solid #D6EDF3;">
							User Name
						</td>
						<td height="30" style="border-bottom:1px solid #D6EDF3">
							Email
						</td>
					</tr>
					<%@users_list.each do |users|%>
					<tr>
						<td height="30" style="color:#307193;border-bottom:1px solid #D6EDF3">
						<%if users && users.present? && users.user_name.present?%><%=users.user_name%><%end%>
						</td>
						<td height="30" style="color:#307193;border-bottom:1px solid #D6EDF3">
						<%if users && users.present? && users.email_address.present?%><%=users.email_address%><%end%>
						</td>
					</tr>
					<%end%><!--do ending here-->
				</table>
			</td>
		</tr>
	<%else%>
		<tr>
		<td style="font-family:Arial, Helvetica, sans-serif; font-size:14px; color:#333333;" height="30">There are no Providers who are due for auto renewal of their accounts today.</td>
		</tr>
	<%end%>
	<!--loop ending here-->
	<!--list of auto renewal user details ending here-->
		
	<tr>
	<td height="10"></td>
	</tr>
	<tr>
		<td height="30" style="color:#307193;"><b>List of Grace Period User Details</b></td>
	</tr>
	<!--loop starting here-->
	<%if @grace_users_list && !@grace_users_list.nil? && @grace_users_list.present? && @grace_users_list!="" && @grace_users_list!=[]%>
		<tr>
			<td style="border:1px solid #D6EDF3" width="500">
				<table cellpadding="0" cellspacing="0" width="90%"  style="margin:0 auto" >
					<tr>
						<td height="30" style="border-bottom:1px solid #D6EDF3;">
							User Name
						</td>
						<td height="30" style="border-bottom:1px solid #D6EDF3">
							Email
						</td>
					</tr>
					<%@grace_users_list.each do |users|%>
					<tr>
						<td height="30" style="color:#307193;border-bottom:1px solid #D6EDF3">
						<%if users && users.present? && users.user_name.present?%><%=users.user_name%><%end%>
						</td>
						<td height="30" style="color:#307193;border-bottom:1px solid #D6EDF3">
						<%if users && users.present? && users.email_address.present?%><%=users.email_address%><%end%>
						</td>
					</tr>
					<%end%><!--do ending here-->
				</table>
			</td>
		</tr>
	<%else%>
		<tr>
		<td style="font-family:Arial, Helvetica, sans-serif; font-size:14px; color:#333333;" height="30">There are no Providers in their grace period today.</td>
		</tr>
	<%end%>
	<!--loop ending here-->
	
	<tr>
	<td height="10"></td>
	</tr>
	
        <tr>
        <td><span style="font-family:Arial, Helvetica, sans-serif; font-size:14px; color:#9c9b9b;padding-top: 15px;display: block;">The Famtivity Team</span></td>
      </tr>
  </table>
    </td>
  </tr>
</table>
