$("#before_import").hide();
<%if (@after==@before)&&@csvlist.count==0%>
$("#loading").hide();
$("#after_import").show();
$("#error_reslt").show();
<%elsif (@after==@before)&&@csvlist.count>0%>
$("#after_import_success").show();
$("#success_reslt").show();
$("#success_reslt").html("Sorry <%=@csvlist.count if !@csvlist.nil? && !@csvlist.count.nil? && @csvlist.count>0%> friend(s) already exist!");
<%else%>
$("#after_import_success").show();
$("#success_reslt").show();
$("#success_reslt").html("You have successfully Imported <%=@now if !@now.nil? %> friend(s)  <%if !@fail.nil? && @fail.present? && @fail!="" && @fail!=0%> <%=@fail%> friend(s) already exist! <%end%> ");
<%end%>

