<%=stylesheet_link_tag "global/pop_up_success", :media => "all"%>
<script language="javascript">
  $(document).ready(function(){
    $("html, body", window.parent.document).animate({ scrollTop: 0 }, 100);
  });
  function closepopup() {
    parent.sponcerPage.hide();
    //parent.parent.location.reload(true);
    parent.parent.location.href += "";
  }
</script>

<div align="center" style="height:230px;">
  <p class="successText" style="padding:60px 0; text-align:center; width:662px;"><%if !@trans_approve.nil?%>
    <%=@trans_approve if !@trans_approve.nil?%><%else%>Sorry!!<%end%></p>
  <div class="clear"></div>
  <a href="javascript:void(0)" onClick="closepopup()" title=""><%= image_tag("/assets/register/ok_btn.png")%></a>
</div>