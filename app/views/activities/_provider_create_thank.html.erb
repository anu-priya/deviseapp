<%=stylesheet_link_tag "global/pop_up_success", :media => "all"%>
<div align="center" style="height:230px;">
  <p class="successText" style="padding:60px 0; font-size: 17px;">You have successfully created your activity!</p>
  <div class="clear"></div>
  <a href="javascript:void(0)" onClick="closepopup_reload();_gaq.push(['_trackEvent', 'Parent Module', 'clickAddActivity','success']);" title=""><%=image_tag("/assets/register/ok_btn.png", :width=>"99", :height=>"38", :style=>"border:0px;")%></a>
</div>

<script type="text/javascript">
$(document).ready(function(){
    $("html, body", window.parent.document).animate({ scrollTop: 0 }, 100);
  });
  function closepopup_reload(){
    parent.newActivityPage.hide();
    parent.location.href += "";  
  }
</script>



