//OnFocus
function focusguestrole(id){
    switch(id){
        case "guest_name":
            if($('#'+id).val()== "Enter Parent Name"){
                $('#'+id).val('');
            }
            break;
	case "guest_email":
            if($('#'+id).val()== "Enter Email"){
                $('#'+id).val('');
            }
            break;
   }
    $('#'+id).css("border","1px solid #9fd8eb");
    $('#'+id).css("color","#444444");
    $('#'+id).focus();
}

//OnBlur
function blurguestrole(id){
    switch(id){
        case "guest_name":
            if($('#'+id).val()== ""){
                $('#'+id).val('Enter Parent Name');
            }
            break;
	case "guest_email":
            if($('#'+id).val()== ""){
                $('#'+id).val('Enter Email');
            }
            break;
   }
    $('#'+id).css("border","1px solid #9fd8eb");
    $('#'+id).css("border","1px solid #BDD6DD");
    $('#'+id).css("color","#999999");
}
</script>
