function dispCheckAgree_free(imgName){

    $("#checkbox_error_agree").css("display","none");   
    if(imgName == 'checkbox_selected_agree'){
        $('#checkbox_selected_agree_free').css('display','none');
        $('#checkbox_error_agree_free').css('display','none');
        $('#checkbox_normal_agree_free').css('display','inline-block');
