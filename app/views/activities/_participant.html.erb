<script type="text/javascript">
  //set automatically go to next textbox
  function movetoNext(current, nextFieldID) {

    if (current.value.length >= current.maxLength) {
      document.getElementById(nextFieldID).focus();
    }
    return true;
  }
  // allow numbers only
function number(evt) {
    evt = (evt) ? evt : window.event;
    var charCode = (evt.which) ? evt.which : evt.keyCode;
    if (charCode > 31 && (charCode < 48 || charCode > 57 ) && (charCode != 46 ) ){
        //alert("Allow only Numbers");
        return false;			
    }
    return true;
}

  function empty_val(){
    $("#nil_policy").show();
    setTimeout("$('#nil_policy').hide();",5000)
  }
	
  function price_set(id,amt,mt_id){

    var price = $("#select_price").val();
    if(price == mt_id){
      var discount_type = $("#select_discount_price").val();
      var payment_day = $("#sessiondays"+discount_type+price).val();
      var comp_day = $("#total_max"+price).val();
      var comp_type = $("#select_type"+price).val();
      var firstVal = payment_day.charAt(0);

      $(".paymentDuration input").css('border','1px solid #CDE0E6');
      $("#scheduleActError").hide();
      $("#scheduleActError").html("");


      if(payment_day!=""){
        if( parseInt(payment_day) > parseInt(comp_day) ){
          $(".paymentDuration input").css('border','1px solid #CDE0E6');
          $("#sessiondays"+discount_type+price).css('border','1px solid #FC8989');
          $("#scheduleActError").show();
          $("#scheduleActError").html('Allow max. no. of duration:'+comp_day);
	  var chk_flag = 'false';
	  $("#set_disp_price").html(" ");
          $("#set_disp_price_val").html("");
          $("#set_disp").html("");
          $("#set_disp_val").html("");
          //alert('Allow max. no. of duration:'+comp_day);
        }
        if( firstVal == 0 ){
          $(".paymentDuration input").css('border','1px solid #CDE0E6');
          $("#sessiondays"+discount_type+price).css('border','1px solid #FC8989');
          $("#scheduleActError").show();
          $("#scheduleActError").html("Please do not use 0 as a first character");
        }
        else if( parseInt(payment_day) <= parseInt(comp_day) ){
          var no_of_value = $("#"+id).val();
          var total_amt = (no_of_value * amt).toFixed(2);

          $("#set_disp_price").html("Price = ");
          $("#set_disp_price_val").html("$"+amt);
          $("#set_disp").html("Quantity of "+no_of_value+" = ");
          $("#set_disp_val").html("$"+total_amt);
        }
        else if( discount_type == "blank" && chk_flag!='false' ){
          var no_of_value = $("#"+id).val();
          var total_amt = (no_of_value * amt).toFixed(2);
          $("#set_disp_price").html("Price = ");
          $("#set_disp_price_val").html("$"+amtt);
          $("#set_disp").html("Your selection for "+no_of_value+" price = ");
          $("#set_disp_val").html("$"+total_amt);
        }
      }
      else{
        $(".paymentDuration input").css('border','1px solid #CDE0E6');
        $("#sessiondays"+discount_type+price).css('border','1px solid #FC8989');

        $("#scheduleActError").show();
        $("#scheduleActError").html('Please enter duration');
      }
    }
  }
  //form popup open
  function show_popup_form()
  {	
	$('#activityform').show();
	$('#content_down').hide();	
  }
    //form popup open
  function hide_popup_form()
  {
	$('#content_down').hide();	
	$('#activityform').hide();	
