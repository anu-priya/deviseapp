<script>
	function closehowitwork()
	{
		parent.BuildNetworkPopup.hide();
	}
	function show_video_parent(){
		$("#vidFrame_1").css("display","none");
		$("#vidFrame_2").css("display","block");
		$("#parent_selected").css("display","block");
		$("#parent_normal").css("display","none");
		$("#provider_normal").css("display","block");
		$("#provider_selected").css("display","none");

		
	}
	function  show_video_provider(){
		
		$("#vidFrame_2").css("display","none");
		$("#vidFrame_1").css("display","block");
		$("#parent_selected").css("display","none");
		$("#parent_normal").css("display","block");
		$("#provider_normal").css("display","none");
		$("#provider_selected").css("display","block");

	}
	var StartVal=1;
	var endVal=7;
	function show_video(getid,url){
	
	for(i=StartVal;i<endVal;i++)
	{
		if(i==getid){
			autoPlayVideo(url,'720','400',i);
		}
	}
}

function autoPlayVideo(vcode, width, height,value) {
				$("#vidFrame_"+value).html('<iframe width="'+width+'" height="'+height+'" src="https://www.youtube.com/embed/'+vcode+'?autoplay=1&loop=1&rel=0&wmode=transparent" style="margin:0;" frameborder="0" allowfullscreen wmode="Opaque"></iframe>');
}

</script>

<div id="how_it_work" class="pop_howit_up" >
	<div class="borde_rows">
		
			<div id="first" >
				<span class="setBoxText" id="haeding_how" align="center" style="top:4px; position:relative;"><b>Activity Group Tutorial</b></span>
				
			</div>
			<div id="close_howit">
				<span style="cursor:pointer"><a onclick="closehowitwork()" title="close"><%=image_tag("/assets/landing/close_button_how.png", :width=>"13", :height=>"13", :align=>"absmiddle")%></a></span>
			</div>
		
		<div class="borde_rows">
			<div id="outer_img">
				 <div  id="vidFrame_2"style="display: none;"  >
					<a class="discover" href="javascript:void(0);" onClick="show_video('2','XQj0hdpvdA8')"><%=image_tag("/assets/landing/parent_video.png", :width=>"720", :height=>"405")%></a> 
				</div>
				<span align="center" style="color: #565455;font-size: 18px;font-weight: bold;"><p>To learn how to build your activity and provider network, take a look at this brief tutorial video below.<br><br></p></span>
				 <div id="vidFrame_1" style="display: block;">
					<a class="discover" href="javascript:void(0);" onClick="show_video('1','tHxN_sXWYnA')"><%=image_tag("/assets/landing/activity_network.png", :width=>"720", :height=>"405")%></a> 
				 </div>
			</div>
		</div>
		<div class="borde_rows">
			<div id="left" class="third" style="margin-left: 70px;text-align: left;">
				<a id="parent_normal" class="discover" href="javascript:void(0)" onClick="closehowitwork()"><%=image_tag("/assets/landing/cancel_button.png", :width=>"101", :height=>"35")%></a>
			</div>
		</div>
	
   
</div>