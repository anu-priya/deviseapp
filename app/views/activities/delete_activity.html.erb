<%=stylesheet_link_tag "global/pop_up_success","global/pop_up","global/delete_schedule", :media => "all"%>

<script language="javascript">
 
  //delete the activity id while clicking the cross image button
  function delete_activity(){
    var delete_id = $('#to_delete').val();
    var action_name = $('#last_action').val();
    var page_no = $('#page').val();   
    var action_del = $("#to_action").val();
    var test_value = "<%=params[:del_action]%>"
    var mode_test = "<%=params[:mode]%>"
    var mode_act_test = "<%=params[:mode_action]%>"
    //loading image while delete the activity
    $('#loading_img').css("display","inline-block");
    $.post("destroy", {"id":delete_id,"last_action":action_name,"page":page_no, "del_action":action_del, "del_mode":mode_test, "del_mode_act":mode_act_test}, function(data){
    $('#loading_img').hide();
    if(data=='true')
    {
    parent.activityDeletePage.hide();
    if (test_value=='index')
    {
    parent.$("#event-act-"+delete_id).remove();
    }
    else if(test_value=='admin_list')
    {
    parent.$("#act-list-"+delete_id).remove();
    }
    else if(test_value=='thumb_index')
    {
    parent.$(".provider-thumb-"+delete_id).remove();
    }
    }
    }, "script");
  }

  function closepopup() {
    parent.$('html, body').css({
    'overflow': 'auto',
    'height': 'auto'
    })
    parent.activityDeletePage.hide();   
  }
</script> 
