<script language="javascript">
function all_s(){
  var totalCheckbox = $("input.s_sched").length;
  if($('input[name=schedule_all]').is(':checked')){
    for (i=1; i<=totalCheckbox; i++){
    //alert(i)
      $('input[name=schedule_'+i+']').attr('checked', true);
    }
    $('#schedule_all').val(1);
  }
  else
  {
    for (i=1; i<=totalCheckbox; i++){
      $('input[name=schedule_'+i+']').attr('checked', false);
    }
    $('#schedule_all').val(0);
  }
}
function single_s(){
  var totalCheckbox = $("input.s_sched").length;
  if($("input.s_sched:checked").length == totalCheckbox){
    $('input[name=schedule_all]').attr('checked', true);
    $('#schedule_all').val(1);
  }
  else{
    $('input[name=schedule_all]').attr('checked', false);
    $('#schedule_all').val(0);
  }
}
function delete_schedule(){
  var act_id ="empty"
  var activity_id="<%=@to_delete%>"

  if($('input[type=checkbox]:checked').length == 0)
  {
    $(".errorDiv").show();
    return false;
  } 
  else{
    $(".errorDiv").hide();
    var del_val = $('#schedule_all').val();
    if(del_val==1){
      //alert("delete all");
      var act_id ="not_empty"
    }

var totalCheckbox = $("input.s_sched").length;
var c_val = '';
    for (i=1; i<=totalCheckbox; i++){
      if($('input[name=schedule_'+i+']').is(':checked')){

       c_val +=  $('#schedule_'+i).val()+",";
      
      }
    } 
    //alert(c_val);
    //alert(act_id);
    var action_name = $('#last_action').val();
    var page_no = $('#page').val();  
    $('#loading_img').css("display","inline-block");
        $.post( "schedule_destroy", {"sch_id":c_val,"act_id":act_id,"activity_id":activity_id,"last_action":action_name,"page":page_no }, function( data ) { }, "script");
    return true;
  }
}

</script>