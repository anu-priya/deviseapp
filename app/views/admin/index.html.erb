  function setCategory(cat){
    var len = cat.length;
	
    for(var i=0;i<len;i++){
      if(len<25){
        $('#filter_by_admin_sub_category_name').html(cat);
      }
      else{
        var val = cat.slice(0,24)
        var value = val+"...";
        $('#filter_by_admin_sub_category_name').html(value);
      }
    }
  }

