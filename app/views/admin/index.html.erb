  function filter_by_admin_date(){    
    var date_from = $("#act_from_alt").val();
    var date_to = $("#act_to_alt").val();
    var view = $("#view_det").val();
    if (Date.parse(date_from) > Date.parse(date_to)) {
      $(".topContainer").css('height','70px');
      $("#dateError").show();
      $("#dateError").html("Invalid date range. Start date cannot be after end date.");
      return false;
    }
    else{
      $(".topContainer").css('height','40px');
      $("#dateError").hide();
      $("#dateError").html("");

      $.get("admin/index_update",{
        "view":view,
        "date_from":date_from,
        "date_to":date_to,
        "cat_zc":"date"
      }, null, "script");
    }
  }
  
