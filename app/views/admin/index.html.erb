<script>
  var availableTags=new Array();
<%if !@activities.nil?%>
  <%@activities.each do |auto_complete|%>
      availableTags.push('<%= auto_complete.activity_name %>');
  <%end%>
<%end%>

  //$(function() {
  //$( "#event_search" ).autocomplete({
  //source: availableTags,
  //maxRows: 12
  //});
  //});

  //this funtion for autocomplete while entered the text by the user dont remove this by rajkumar
  $(function() {
    $( "#event_search" ).autocomplete({
      minLength: 1,
      source: function(request, response) {
        var results = $.ui.autocomplete.filter(availableTags, request.term);
        response(results.slice(0, 8));
      }
    });
  });

  //clear autocomplete input..
  function clearAutoCompleteInput() {
    $("#event_search").val('');
  }
</script>
