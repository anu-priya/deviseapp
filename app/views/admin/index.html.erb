  function raise_a_flag(activity_id,rais_val){
    var report = document.getElementById('raiseFlag_'+ activity_id).value;
    var view = $("#view_det").val();

    $.ajax({
      type: "POST",
      url: "admin/raise_flag_update",
      data: {
        "view":view,
        "approve":rais_val,
        "desc":report,
        "activity_id":activity_id
      },
      success: function(data){

      },
      dataType: "json"
    });

    document.getElementById('raise_popup_'+ activity_id).style.display="none";
    if ((rais_val==true) || (rais_val =="true")){
      $('#approve_'+activity_id).replaceWith('<span id="approve_'+activity_id+'"><img src="/assets/admin/tig_image.png" class="tig_image"><span class="ftSz12" style="left: 4px; position: relative; top: 6px;vertical-align: top;">Approved</span></span>');
      $('#raised_'+activity_id).replaceWith('<span id="raised_'+activity_id+'"><a href="javascript:void(0)" id='+activity_id+' title="" style="margin-left:9px;" onclick="showPop('+activity_id+','+activity_id+');"><img src="/assets/admin/raise_a_flag_button.png"></a></span>');
    }else{
      $('#approve_'+activity_id).replaceWith('<span id="approve_'+activity_id+'"><a href="javascript:void(0)" title="" onclick="raise_a_flag('+activity_id+',true);"><img src="/assets/admin/approve_button.png"></a></span>');
      $('#raised_'+activity_id).replaceWith('<span id="raised_'+activity_id+'"><img src="/assets/admin/tig_image.png" class="tig_image"><span class="ftSz12" style="left: 4px; position: relative; top: 6px;vertical-align: top;">Raised</span></span>');
    }
  }

