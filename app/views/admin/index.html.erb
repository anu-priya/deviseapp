$(function() {
	$( "#act_from" ).datepicker({
		  showOn : "button",
		  buttonImage : "/assets/create_new_activity/date_icon.png",
		  buttonImageOnly : true,
		  dateFormat : "m/d/yy",
		  altField : "#act_from_alt",
