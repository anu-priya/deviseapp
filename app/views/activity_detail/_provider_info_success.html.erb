<%=stylesheet_link_tag "global/pop_up_success","global/pop_up", :media => "all"%>
<script language="javascript">
$(document).ready(function(){
	$("html, body", window.parent.document).animate({ scrollTop: 0 }, 100);
});
function closepopup() {		
	parent.providerinfoPage.hide();
	parent.parent.location.href += "";
	//parent.parent.location.reload(true);		
}	
</script>
<div class="closeButton" style="float:right; margin:4px 20px 0 0;">
	<a href="javascript:void(0)" onClick="closepopup()" title="">  <%=image_tag("/assets/global/pop_up/button_close.png", :width=>"15", :height=>"15", :style=>"border:0px;")%> </a>
</div>
<div align="center" style="height:230px;">

  <p class="successText fts16" style="padding:60px 0;"><b>Your Email has been sent!.</b></p>
  <div class="clear"></div>
  <a href="javascript:void(0)" onClick="closepopup()" title=""><%=image_tag("/assets/register/ok_btn.png", :width=>"99", :height=>"38")%></a>
</div>



