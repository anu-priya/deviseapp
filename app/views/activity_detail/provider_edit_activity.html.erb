                                  <script type="text/javascript">
                                    //Tags concept
                                    var i = 0;
                                    $(document).ready(function(){
                                <%if !@activity.nil? && @activity.camps == true%>
                                      $("#class_link_1").hide();
                                      $("#class_link_2").show();
                                      $("#class_link").val(1);
                                <%end%>
                                <%if !@activity.nil? && @activity.special_needs == true%>
                                      $("#special_link_2").show();
                                      $("#special_link_1").hide();
                                      $("#special_link").val(1);
                                <%end%>

                                    if($("#edit-tag-end-value") && $("#edit-tag-end-value").val() != "" && $("#edit-tag-end-value").val() != "undefined")
                                    {
                                      i = parseInt($("#edit-tag-end-value").val());
                                      if(isNaN(i))
                                      {
                                        i=0;
                                      }
                                    }
                                  });
                                  $("#edit-tag-txt").live("keydown",function(event){
                                    $("#tag_error").html("");
                                    $(".errorDiv").css("display","none");
                                    if(event.keyCode == 13)
                                    {
                                      event.preventDefault();
                                      var txt_tag = this.value.trim();
                                      var txt_tag_value = this.value.trim();
                                      if(txt_tag != "" && txt_tag != "undefined" && (/^\d*[a-zA-Z-]{1,}\d*/.test(txt_tag)))
                                      {
                                        var chk_arr = new Array();
                                        $(".taginput").each(function( index ) {
                                          chk_arr.push(this.value);
                                        });
                                        var lowerCaseItems = $.map(chk_arr, function(n,i){return n.toLowerCase();});
                                        if(lowerCaseItems.indexOf(txt_tag.toLowerCase()) != -1)
                                        {
                                          $("#tag_error").html("You can't add the same tag more than once");
                                          $(".errorDiv").css("display","block");
                                        }
                                        else
                                        {
                                          i = parseInt(i) + 1;
                                          $("<div class='vR' id='edit_tagdiv_"+i+"'><span class='vN vP'><div class='vT'>"+txt_tag+"</div><div class='vM cls' onclick=cls_tag("+i+") title='Remove Tag'></div></span><input name='to' type='hidden' value='"+txt_tag_value+"' id='edit_taginput_"+i+"' class='taginput' /></div>").appendTo($( "#tag-keyword" ));
                                          $("#edit-tag-txt").val("");
                                          assign_tag_value();
                                        }
                                      }
                                    }
                                  });

                                  $("#edit-tag-txt").live("blur",function(event){
                                    $("#tag_error").html("");
                                    $(".errorDiv").css("display","none");
                                    var txt_tag = this.value.trim();
                                    var txt_tag_value = this.value.trim();
                                    if(txt_tag != "" && txt_tag != "undefined" && (/^\d*[a-zA-Z-]{1,}\d*/.test(txt_tag)))
                                    {
                                      var chk_arr = new Array();
                                      $(".taginput").each(function( index ) {
                                        chk_arr.push(this.value);
                                      });
                                      var lowerCaseItems = $.map(chk_arr, function(n,i){return n.toLowerCase();});
                                      if(lowerCaseItems.indexOf(txt_tag.toLowerCase()) != -1)
                                      {
                                        $("#tag_error").html("You can't add the same tag more than once");
                                        $(".errorDiv").css("display","block");
                                      }
                                      else
                                      {
                                        if(/,/.test(txt_tag_value)){
                                          var tagsplit=txt_tag_value.split(',');
                                          for(var a=0;a<tagsplit.length;a++){
                                            $("<div class='vR' id='edit_tagdiv_"+i+"'><span class='vN vP'><div class='vT'>"+tagsplit[a]+"</div><div class='vM cls' onclick=cls_tag("+i+") title='Remove Tag'></div></span><input name='to' type='hidden' value='"+tagsplit[a]+"' id='edit_taginput_"+i+"' class='taginput' /></div>").appendTo($( "#tag-keyword" ));
                                            $("#edit-tag-txt").val("");
                                            assign_tag_value();
                                          }
                                        }
                                        else{
                                          i = parseInt(i) + 1;
                                          $("<div class='vR' id='edit_tagdiv_"+i+"'><span class='vN vP'><div class='vT'>"+txt_tag+"</div><div class='vM cls' onclick=cls_tag("+i+") title='Remove Tag'></div></span><input name='to' type='hidden' value='"+txt_tag_value+"' id='edit_taginput_"+i+"' class='taginput' /></div>").appendTo($( "#tag-keyword" ));
                                          $("#edit-tag-txt").val("");
                                          assign_tag_value();
                                        }
          
                                      }
                                    }
                                  });

                                  $("#edit-tag-txt").autocomplete({
                                    source: "/activity_tags",
                                    select: function( event, ui ) {
                                      if(event.keyCode != 13)
                                      {
                                        var txt_tag = ui.item.value.trim();
                                        var txt_tag_value = ui.item.value.trim();
                                        if(txt_tag != "" && txt_tag != "undefined" && (/^\d*[a-zA-Z-]{1,}\d*/.test(txt_tag)))
                                        {
                                          var chk_arr = new Array();
                                          $(".taginput").each(function( index ) {
                                            chk_arr.push(this.value);
                                          });
                                          var lowerCaseItems = $.map(chk_arr, function(n,i){return n.toLowerCase();});
                                          if(lowerCaseItems.indexOf(txt_tag.toLowerCase()) != -1)
                                          {
                                            $("#tag_error").html("You can't add the same tag more than once");
                                            $(".errorDiv").css("display","block");
                                          }
                                          else
                                          {
                                            i = parseInt(i) + 1;
                                            $("<div class='vR' id='edit_tagdiv_"+i+"'><span class='vN vP'><div class='vT'>"+txt_tag+"</div><div class='vM cls' onclick=cls_tag("+i+") title='Remove Tag'></div></span><input name='to' type='hidden' value='"+txt_tag_value+"' id='edit_taginput_"+i+"' class='taginput' /></div>").appendTo($( "#tag-keyword" ));
                                            $("#edit-tag-txt").val("");
                                            assign_tag_value();
                                          }
                                        }
                                      }
                                      return false;
                                    }
                                  }).keyup(function (e) {
                                    if(e.which == 13) {
                                      $(".ui-menu").hide();
                                    }
                                  });
                                  function cls_tag(id){
                                    $('#edit_tagdiv_'+id).remove();
                                    assign_tag_value();
                                  }
  
                                  function assign_tag_value(){
                                    var arr=new Array();
                                    $(".taginput").each(function( index ) {
                                      arr.push(this.value);
                                    });
                                    $("#edit-keyword-tags").val(arr);
                                    $("#edit-tag-txt").focus();
                                  }

                                  function getchk_link(id){
                                    if (id=="class_link_2")
                                    {
                                      $("#class_link_1").hide();
                                      $("#class_link_2").show();
                                      $("#class_link").val(1);
                                    }
                                    else if (id=="class_link_1")
                                    {
                                      $("#class_link_2").hide();
                                      $("#class_link_1").show();
                                      $("#class_link").val(0);
                                    }
                                    else if (id=="special_link_1")
                                    {
                                      $("#special_link_1").show();
                                      $("#special_link_2").hide();
                                      $("#special_link").val(0);
                                    }
                                    else if (id=="special_link_2")
                                    {
                                      $("#special_link_2").show();
                                      $("#special_link_1").hide();
                                      $("#special_link").val(1);
                                    }

                                  }
                                  </script>
