<div class="formListDetails" id="formgrid">
	<table width="100%" border="0" cellpadding="0" cellspacing="0">
		<tr class="tableHeding" height="30">
			<td width="25%" class="pleft" align="left" valign="middle">Field Name</td>
			<td width="25%" align="left" valign="middle">Field Type</td>
			<td width="35%" align="left" valign="middle">Help Text</td>
			<td width="15%" class="pright" align="left" valign="middle">Action</td>
		</tr>		
		<%if !@get_field.nil? && @get_field!='' && @get_field.length > 0%>	
			<%k=1%>
		<%@get_field.each do |qval|%>
			<%if !qval["question_id"].nil? && qval["question_id"]!='' && qval["question_id"].present?%>
			<%@question=get_question_details(qval["question_id"]) %>
			<tr class="tableDetails" id="<%=k%>" height="25">
				<td width="25%" class="pleft" align="left" valign=" middle"><%=@question[0]["field_name"]%></td>
				<td width="25%" align="left" valign=" middle"><%=@question[0]["type_name"]%></td>
				<td width="35%" align="left" valign=" middle"><%=@question[0]["help_text"]%></td>
				<td width="15%" class="pright" colspan="2" align="left" valign=" middle">
					<table  border="0" cellpadding="0" cellspacing="0">
						<tr class="action">
							<td align="left" valign=" middle"><a href="javascript:void(0);" title="Edit" onclick="edit_field(<%=qval["question_id"]%>); _gaq.push(['_trackEvent','Provider Module', 'clickForm','Edit','<%=qval["question_id"]%>']);">Edit</a></td>
							<td align="left" valign=" middle"> &nbsp;<span class="iconList">|</span> &nbsp;</td>
							<td align="left" valign=" middle"><a href="javascript:void(0);" title="Delete" onclick="delete_field(<%=qval["question_id"]%>,<%=k%>); _gaq.push(['_trackEvent','Provider Module', 'clickForm','Delete','<%=qval["question_id"]%>']);">Delete</a></td>
						</tr>
					</table>
				</td>
			</tr>
			<tr class="tableDetails" style="display:none;" height="30" id="norecord">
				<td colspan="4" align="center" valign=" middle">No fields added in the form</td>
			</tr>
			<%k=k+1%>
			<%else%>
			<tr class="tableDetails">
				<td colspan="4" align="center" valign="middle" height="28">No fields added in the form</td>
			</tr>
			<%end%>
		<%end%>
		<%else%>		
		<tr class="tableDetails">
			<td colspan="4" align="center" valign=" middle" height="28">No fields added in the form</td>
		</tr>
		<%end%>		
	</table>
</div>