      <div class="innerContainer">
        <div class="headTab">
          <div class="heading" style="width:960px;">
            <h1 class="headCont">
              <% if !current_user.nil? && !current_user.user_name.nil? && @page_show != "price"%>
		Become a Provider
	    <%elsif  request.fullpath=="/provider-pricing-plan"%>
		  Sign-up as Provider <span class="fts14" style="position: relative;top: -3px;">(...& get <span class="ftcRed">$20</span> Credit!)</span>
              <%else%>
                <%if @page_show != "provider" && @page_show != "price" && @page_show != "ptr"%>
                  Sign-up as Parent <span class="fts14" style="position: relative;top: -3px;">(...& get <span class="ftcRed">$20</span> Credit!)</span>
                <%elsif @page_show =="provider" || @page_show == "price"%>
                  Sign-up as Provider <span class="fts14" style="position: relative;top: -3px;">(...& get <span class="ftcRed">$20</span> Credit!)</span>
	      <%elsif @page_show == "ptr"%>
	        Register as Partner
                <%end%>
              <% end %>
            </h1>
          </div>
          <div class="closeButton">
            <%if !@log_value.nil? && @log_value.present? && @log_value=='reld'%>
             <%= link_to image_tag("/assets/global/pop_up/button_close.png", :border => 0), root_path, :target => "_parent" %>
            <%else%>
            <a href="javascript:void(0)" onClick="closepopup()" title=""> <img src="/assets/global/pop_up/button_close.png" alt="" width="15" height="15" /> </a>
            <%end%>
          </div>
        </div>

        <div id="register_outerContainer">
          <div id="curator_message" style="display:none;text-align: center;height: 430px;width: 600px;margin: 0 auto;font-size: 16px;color: #000;" >
            <div style="text-align:center;padding: 25px;line-height: 22px;">
            Your email address is already in use. If you did not create  an account, it is likely that Famtivity has added this email as a provider account. </br>Click on the button below to proceed and take control of your account.
            </div>
            <div style="border-top:1px solid #d7dadb; margin:0 auto; width: 100%; height:25px;">&nbsp;</div>
            <div style="width:300px;height:240px;margin:0 auto;border: 1px solid #d7dadb;">
              <div style="font-weight: bold;color: #5099ac;margin-top: 20px;margin-bottom: -15px;font-size: 21px;">Provider</div>
            <div style="text-align:center;padding: 35px;line-height: 22px;"><img src="/assets/global/curator_image.png" alt=""  />
            </div>

            <a href="javascript:void(0)" onClick="curatormessage()" title=""> <img src="/assets/global/switch_provider.png" alt=""  /> </a>
            </div>
          </div>

          <!-- Parent Register Page -->
          <div id="ParentRegister" <%if @curator_val.nil? && @page_show != "provider" && @page_show != "price" && @page_show != "ptr" && @pro_invite.nil? && !@pro_invite.present?%>style="display:block;"<%else%> style="display:none;"<%end%>>
         <%if request.fullpath=="/provider-pricing-plan"%>
	   <%= render :partial=>'provider_register_profile' %>
	<%else%>
            <%= render :partial =>'user_register' %>
	    <% end %>
            <div class="clear"></div>
          </div>

          <!-- Provider Register profile plan Page -->
          <div id="provider_plan" <%if @curator_val.nil? && @page_show == "provider" || @page_show == "price"|| !@pro_invite.nil? && @pro_invite.present?%>style="display:block;"<%else%> style="display:none;"<%end%>>
	  <%if @u_exists && @u_exists.present? && !@u_exists.nil?%>
	  <%= render :partial =>'existing_user_error_msg' %>
	  <%else%>
            <%= render :partial =>'provider_register_profile' %>
	  <%end%>
            <div class="clear"></div>
          </div>

          <!-- Provider Register market(free) page -->
          <div id="provider_market" style="display:none;">
            <%= render :partial =>'provider_market' %>
            <div class="clear"></div>
          </div>

          <!-- Provider Register sponsor page -->
          <div id="provider_sponsor" style="display:none;">
            <%= render :partial =>'provider_sponsor' %>
            <div class="clear"></div>
          </div>

          <!-- Provider Register sell page -->
          <div id="provider_sell" style="display:none;">
            <%= render :partial =>'provider_sell' %>
            <div class="clear"></div>
          </div>


        </div>
      </div>
      <div class="clear"></div>

    </div>
    <div class="clear"></div>
