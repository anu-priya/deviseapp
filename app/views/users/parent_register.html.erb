    $('#sell_year_card_'+value).val(yvalue);
    $('#sell_field2_'+value).html(yvalue);
    $('#sell_field2_'+value).css('color','#444444');
    //$("#EditdropDownDiv3").css("display","none");
	
  }


</script>

