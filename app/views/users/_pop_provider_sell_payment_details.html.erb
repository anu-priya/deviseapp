  function validateName(elementValue){
    var alphaExp = /^[a-zA-Z ]*$/;
    return alphaExp.test(elementValue);
  }
  function validateAName(elementValue){
    var alphaExp =  /^[a-zA-Z. ]*$/;
    return alphaExp.test(elementValue);
