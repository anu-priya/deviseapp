<script type="text/javascript">
  var page = "event_more";

  var headerPart2_width = $(".headerPart2").width();
  var favorite_bar_width = $("#favorite_bar").width();

  var inner_favorite_bar_width = ($("#inner_favorite_bar li").length)*(85+2);
  $("#inner_favorite_bar").css("width",inner_favorite_bar_width+"px");

  var favorite_bar_right_arrow_diff = favorite_bar_width-38;
  $(".favorite_bar_right_arrow a").css("margin-left",favorite_bar_right_arrow_diff+"px");

  var favorite_bar_left_arrow_img = false;
  var favorite_bar_right_arrow_img = false;

  if(inner_favorite_bar_width > favorite_bar_width){
    //$(".favorite_bar_right_arrow a").css("display","block");
    $(".favorite_bar_right_arrow a").css("display","none");
    favorite_bar_right_arrow_img = true;
  }

  var favorite_bar_pos = 0;
  var favorite_bar_flag;

  var favorite_bar_content_width = inner_favorite_bar_width;
  var favorite_bar_max_width = favorite_bar_width;

  $(window).bind('resize', function () {
    var favorite_bar_width = $("#favorite_bar").width();
    var favorite_bar_right_arrow_diff = favorite_bar_width-38;
    $(".favorite_bar_right_arrow a").css("margin-left",favorite_bar_right_arrow_diff+"px");

    favorite_bar_max_width = favorite_bar_width;
  });

  function favoriteBarRight(){
    if(favorite_bar_flag){
      if((favorite_bar_pos + (favorite_bar_content_width-favorite_bar_max_width)) > 0){
        favorite_bar_pos--;
        $("#inner_favorite_bar").css("margin-left",favorite_bar_pos);
        $(".favorite_bar_left_arrow a").css("display","block");
        favorite_bar_left_arrow_img = true;
        setTimeout("favoriteBarRight",10);
      }
      else{
        $(".favorite_bar_right_arrow a").css("display","none");
        favorite_bar_right_arrow_img = false;
      }
    }
  }

  function favoriteBarLeft(){

    if(favorite_bar_flag){
      if((favorite_bar_pos + (favorite_bar_content_width-favorite_bar_max_width)) < (favorite_bar_content_width-favorite_bar_max_width)){
        favorite_bar_pos++;
        $("#inner_favorite_bar").css("margin-left",favorite_bar_pos);
        $(".favorite_bar_right_arrow a").css("display","block");
        favorite_bar_right_arrow_img = true;
        setTimeout("favoriteBarLeft",10);
      }
      else{
        $(".favorite_bar_left_arrow a").css("display","none");
        favorite_bar_left_arrow_img = false;
      }
    }
  }

  function favoriteBarArrowShow(){
    if(favorite_bar_left_arrow_img){

      $(".favorite_bar_left_arrow a").css("display","block");
    }
    if(favorite_bar_right_arrow_img){
      $(".favorite_bar_right_arrow a").css("display","block");
    }
  }

  function favoriteBarArrowHide(){
    if(favorite_bar_left_arrow_img){
      $(".favorite_bar_left_arrow a").css("display","none");
    }
    if(favorite_bar_right_arrow_img){
      $(".favorite_bar_right_arrow a").css("display","none");
    }
  }

  $("#favorite_bar").mouseover(function(){favoriteBarArrowShow();});
  $("#favorite_bar").mouseout(function(){favoriteBarArrowHide();});
</script>
